#include <bits/stdc++.h>
using namespace std;

vector<int> Generar Vectores (vector<int> A);
vector<int> CuentoVectores (vector<int> A, <vector> int B);
void MostrarVectores (vector<int> A, vector<int> B);

int main(){
vector<int>A;
vector<int> B;
A= Generar Vectores (A);
B=CuentoVectores (A, B);
Mostrar Vectores (A,B);

return 0;
}
vector<int> GenerarVectores (vector<int> A) {
Srand (time (NULL));
for (int i=0; i < 100; i++) {
A.push_back (rand()% 100); }
return A;
}
vector<int> CuentoVectores (vector<int> A, <vector> int B){
for (int i=0; i<A.size(); i++) {
if(A[i]%2==0){
 if(A[i]>20){
 B.push_back(A[i]*10);
}
if(A[i]<60){
B.push_back(A[i]-30)
   }
 }
}
return B;
}
